{
  "version": 2,
  "builds": [
    {
      "src": "src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/trend",
      "dest": "src/public/trend.html"
    },
    {
      "src": "/(.*)",
      "dest": "src/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
