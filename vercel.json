{
  "version": 2,
  "routes": [
    {
      "src": "/trend",
      "dest": "/src/public/trend.html"
    },
    {
      "src": "/",
      "dest": "/src/public/index.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/src/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/public/$1"
    }
  ]
}
